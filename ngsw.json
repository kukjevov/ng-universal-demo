{
    "index": "/index.html",
    "assetGroups":
    [
        {
            "name": "app",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources":
            {
                "files":
                [
                    "/index.html"
                ],
                "versionedFiles":
                [
                    "/dist/*.*",
                    "!/dist/server.js"
                ]
            }
        },
        {
            "name": "localizations",
            "installMode": "prefetch",
            "updateMode": "prefetch",
            "resources":
            {
                "files":
                [
                    "/config/**"
                ]
            }
        },
        {
            "name": "externals",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources":
            {
                "files":
                [
                    "/dist/css/**"
                ]
            }
        }
    ],
    "dataGroups": 
    [
        {
            "name": "api-freshness",
            "urls": 
            [
                "/api/**"
            ],
            "cacheConfig": 
            {
                "maxSize": 200,
                "maxAge": "6h",
                "timeout": "90s",
                "strategy": "freshness"
            }
        }
    ]
}