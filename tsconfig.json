{
    "compilerOptions":
    {
        "moduleResolution": "node",
        "module": "es2015",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "lib": 
        [
            "es2016",
            "dom"
        ],
        "types": 
        [
            "node",
            "systemjs"
        ],
        "typeRoots": 
        [
            "node_modules/@types"
        ]
    },
    "exclude": 
    [
        "node_modules",
        "app/main.server.aot.ts"
    ]
}