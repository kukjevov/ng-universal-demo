{
    "name": "ng-universal-demo",
    "version": "1.0.0",
    "description": "Angular universal sample application",
    "scripts":
    {
        "build": "webpack --env.dll && webpack --env.ssr",
        "build:aot": "npm run ngc -- -p tsconfig.browser.json",
        "build:min": "webpack --env.aot --env.prod && webpack --env.aot --env.ssr",
        "build:gulp": "gulp build",
        "build:gulp:aot": "gulp build:aot",
        "prebuild": "npm run build:gulp",
        "prebuild:aot": "npm run prebuild:min",
        "prebuild:min": "npm run clean && npm run build:gulp:aot",
        "postbuild:aot": "npm run postbuild:min",
        "postbuild:min": "npm run clean:tmp",
        "build:prepare:dev": "webpack --config webpack.config.dev.js",
        "clean": "rimraf wwwroot testResults && npm run clean:tmp",
        "clean:tmp": "rimraf app.aot",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "app-config:w": "gulp watch-config",
        "scss": "gulp compile-scss",
        "tsc": "tsc",
        "ngc": "ngc",
        "update-versions": "nrs versionDeployment.config.json",
        "postinstall": "npm run build:prepare:dev && npm run build",
        "server": "node server.js",
        "start": "npm run build:gulp && concurrent \"npm run app-config:w\" \"npm run server -- --webpack\"",
        "test": "karma start karma.conf.js"
    },
    "engines":
    {
        "node": ">=6.0.0"
    },
    "author": "Marek Bukovsk√Ω<marek.bukovsky@asseco-ce.com>",
    "license": "MIT",
